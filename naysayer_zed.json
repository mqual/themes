{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Naysayer",
  "author": "Nick Aversano (Ported)",
  "themes": [
    {
      "name": "Naysayer",
      "appearance": "dark",
      "style": {
        "background": "#061311",
        "editor.background": "#061311",
        "editor.foreground": "#e5caa8",
        "text": "#e5caa8",
        "text.muted": "#688c92",
        "text.accent": "#32d4ac",
        "editor.line_number": "#132c29",
        "editor.active_line_number": "#ffffff",
        "editor.active_line.background": "#132c29",
        "editor.highlighted_line.background": "#132c29",
        "status_bar.background": "#061311",
        "tab_bar.background": "#061311",
        "tab.inactive_background": "#061311",
        "tab.active_background": "#132c29",
        "border": "#132c29",
        "border.focused": "#32d4ac",
        "error": "#ff1a1a",
        "warning": "#ffb515",
        "info": "#32d4ac",
        "hint": "#85e0d6",
        "search.match_background": "#32d4ac66",
        "players": [
          {
            "cursor": "#32d4ac",
            "selection": "#32d4ac3d",
            "background": "#061311"
          }
        ],
        "syntax": {
          "comment": {
            "color": "#4ac245",
            "font_style": "italic"
          },
          "comment.doc": {
            "color": "#4ac245",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ffffff"
          },
          "keyword.control": {
            "color": "#ffffff"
          },
          "keyword.operator": {
            "color": "#ffffff"
          },
          "operator": {
             "color": "#98eba0"
          },
          "function": {
            "color": "#e5caa8"
          },
          "function.method": {
            "color": "#e5caa8"
          },
          "type": {
            "color": "#e5caa8"
          },
          "type.builtin": {
            "color": "#e5caa8"
          },
          "variable": {
            "color": "#e5caa8"
          },
          "variable.special": {
             "color": "#85e0d6"
          },
          "variable.parameter": {
             "color": "#e5caa8"
          },
          "string": {
            "color": "#32d4ac"
          },
          "string.escape": {
            "color": "#4ac245"
          },
          "string.regexp": {
            "color": "#85e0d6"
          },
          "number": {
            "color": "#85e0d6"
          },
          "boolean": {
            "color": "#85e0d6"
          },
          "constant": {
            "color": "#85e0d6"
          },
          "constant.numeric": {
            "color": "#85e0d6"
          },
          "constant.builtin": {
            "color": "#85e0d6"
          },
          "punctuation": {
            "color": "#98eba0"
          },
          "punctuation.bracket": {
            "color": "#98eba0"
          },
          "punctuation.delimiter": {
            "color": "#98eba0"
          },
          "punctuation.special": {
             "color": "#ffffff"
          },
          "tag": {
            "color": "#32d4ac"
          },
          "attribute": {
             "color": "#32d4ac"
          },
          "link_uri": {
             "color": "#85e0d6",
             "font_style": "underline"
          }
        }
      }
    }
  ]
}
