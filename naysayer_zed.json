{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Naysayer",
  "author": "Nick Aversano (Ported)",
  "themes": [
    {
      "name": "Naysayer",
      "appearance": "dark",
      "style": {
        "background": "#062329",
        "editor.background": "#062329",
        "editor.foreground": "#d1b897",
        "text": "#d1b897",
        "text.muted": "#44b340",
        "editor.line_number": "#126367",
        "editor.active_line.background": "#0b3335",
        "status_bar.background": "#062329",
        "tab_bar.background": "#062329",
        "tab.inactive_background": "#04181c",
        "tab.active_background": "#062329",
        "border": "#126367",
        "error": "#ff0000",
        "warning": "#ffaa00",
        "info": "#2ec09c",
        "hint": "#44b340",
        "search.match_background": "#126367",
        "players": [
          {
            "cursor": "#ffffff",
            "selection": "#0000ff",
            "background": "#062329"
          }
        ],
        "syntax": {
          "comment": {
            "color": "#44b340",
            "font_style": "italic"
          },
          "comment.doc": {
            "color": "#44b340",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ffffff"
          },
          "keyword.control": {
            "color": "#ffffff"
          },
          "keyword.operator": {
            "color": "#ffffff"
          },
          "operator": {
             "color": "#ffffff"
          },
          "function": {
            "color": "#ffffff"
          },
          "function.method": {
            "color": "#c1d1e3"
          },
          "type": {
            "color": "#8cde94"
          },
          "type.builtin": {
            "color": "#ffffff"
          },
          "variable": {
            "color": "#c1d1e3"
          },
          "variable.special": {
             "color": "#c1d1e3"
          },
          "variable.parameter": {
             "color": "#c1d1e3"
          },
          "string": {
            "color": "#2ec09c"
          },
          "string.escape": {
            "color": "#8cde94"
          },
          "string.regexp": {
            "color": "#2ec09c"
          },
          "number": {
            "color": "#7ad0c6"
          },
          "boolean": {
            "color": "#ffffff"
          },
          "constant": {
            "color": "#7ad0c6"
          },
          "constant.numeric": {
            "color": "#7ad0c6"
          },
          "constant.builtin": {
            "color": "#ffffff"
          },
          "punctuation": {
            "color": "#8cde94"
          },
          "punctuation.bracket": {
            "color": "#8cde94"
          },
          "punctuation.delimiter": {
            "color": "#8cde94"
          },
          "punctuation.special": {
             "color": "#8cde94"
          },
          "tag": {
            "color": "#ffffff"
          },
          "attribute": {
             "color": "#c1d1e3"
          },
          "link_uri": {
             "color": "#2ec09c",
             "font_style": "italic"
          }
        }
      }
    }
  ]
}
